			<button href="#task-modal" role="button" class="btn-tasks btn btn-primary btn-small" data-toggle="modal">See tasks</button>
		</h1>
		<hr/>

		<section>
			<div class="container marketing">

				<!-- Three columns of text below the carousel -->
				<div class="articles row">
					<article class="col-lg-4 clearfix">
						<img class="image-feature" src="<%- img('photos/jakobThumb200Square.jpg') %>" width="200" >
